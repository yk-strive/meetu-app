#personaldata_page {
  overflow-y: scroll;
}
#personaldata_page scroll-view {
    height: calc(100% - 128rpx);
}
#personaldata_page .avatar_box {
    position: relative;
    width: 100%;
    height: 346rpx;
}
#personaldata_page .avatar_box image.avatar_bg {
      width: 326rpx;
      height: 100%;
}
#personaldata_page .avatar_box .avatar {
      width: 180rpx;
      height: 180rpx;
      overflow: hidden;
      top: 53%;
}
#personaldata_page .form_list {
    margin: 80rpx 78rpx;
}
#personaldata_page .form_list .cu-form-group + .cu-form-group {
      border-top: none;
}
#personaldata_page .form_list .cu-form-group {
      padding: 30rpx 0 0;
      background-color: transparent;
      border-bottom: 2rpx solid #593DA5;
}
#personaldata_page .form_list .cu-form-group .title {
        color: #735CB1;
        font-size: 32rpx;
        letter-spacing: 4rpx;
}
#personaldata_page .form_list .cu-form-group input {
        height: 90rpx;
        font-size: 32rpx;
        color: #FFFFFF;
}
#personaldata_page .form_list .cu-form-group .right {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
        position: relative;
        padding-right: 40rpx;
        overflow: hidden;
        line-height: 100rpx;
}
#personaldata_page .form_list .cu-form-group .right::after {
          font-family: cuIcon;
          display: block;
          content: "\E6A3";
          position: absolute;
          font-size: 34rpx;
          color: #8799a3;
          line-height: 100rpx;
          width: 60rpx;
          text-align: center;
          top: 0;
          bottom: 0;
          right: -20rpx;
          margin: auto;
}
#personaldata_page .submit_box {
    position: relative;
    height: 160rpx;
}
#personaldata_page .submit_box image,
    #personaldata_page .submit_box button {
      width: 110rpx;
      height: 110rpx;
}
#personaldata_page .submit_box button.submit {
      opacity: 0;
}
.bg-gradual-blue {
  background-image: -webkit-linear-gradient(45deg, #0081ff, #1cbbb4);
  background-image: linear-gradient(45deg, #0081ff, #1cbbb4);
  color: #ffffff;
}

