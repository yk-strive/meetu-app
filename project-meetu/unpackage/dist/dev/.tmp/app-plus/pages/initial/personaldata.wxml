<view class="bg_page_2" id="personaldata_page"><custom-nav vue-id="1" isBack="{{isEditInfo?true:false}}" textTitle="{{isEditInfo?'修改个人信息':''}}" bind:__l="__l"></custom-nav><scroll-view class="wrap_heihgt" scroll-y="true"><view class="avatar_box"><image class="abs-center avatar_bg" src="../../static/meetu-img/face.png" mode="aspectFill"></image><view class="avatar abs-center round"><image class="wh-100" src="{{userInfo?userInfo.headimgurl:''}}" mode="aspectFill" data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" bindtap="__e"></image></view></view><form action data-event-opts="{{[['submit',[['submitInfo',['$event']]]]]}}" bindsubmit="__e"><view class="form_list"><view class="cu-form-group"><view class="title">昵称</view><input class="text-right" name="nickname" value="{{userInfo.nickname}}"/></view><view class="cu-form-group"><view class="title">性别<text class="text-sm">（修改后不可更改）</text></view><view data-event-opts="{{[['tap',[['togglePicker',['selector']]]]]}}" class="right text-right text-white text-lg" bindtap="__e">{{sex}}</view><block wx:if="{{selectList.length!=0}}"><w-picker class="vue-ref" vue-id="2" mode="selector" defaultVal="{{sex}}" themeColor="#f00" selectList="{{selectList}}" data-ref="selector" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></block></view><view class="cu-form-group"><view class="title">生日</view><view data-event-opts="{{[['tap',[['togglePicker',['date']]]]]}}" class="right text-right text-white text-lg" bindtap="__e">{{pickerBirthdayInfo.result}}</view><w-picker class="vue-ref" vue-id="3" mode="date" startYear="1959" endYear="2030" defaultVal="{{pickerBirthdayInfo.checkArr}}" current="{{false}}" disabledAfter="{{true}}" themeColor="#f00" data-ref="date" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view><view class="cu-form-group"><view class="title">地区</view><view data-event-opts="{{[['tap',[['togglePicker',['region']]]]]}}" class="right text-right text-white text-lg" bindtap="__e">{{pickerRegionInfo.result}}</view><w-picker class="vue-ref" vue-id="4" mode="region" defaultVal="{{pickerRegionInfo.checkArr}}" areaCode="{{pickerRegionInfo.checkValue}}" hideArea="{{true}}" timeout="{{true}}" data-ref="region" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view><view class="cu-form-group"><view class="title">邀请码<text class="text-sm">（选填）</text></view><input class="text-right" value="" name="invitecode"/></view></view><view class="submit_box"><image class="abs-h-center" src="../../static/meetu-img/sure.png" mode="widthFix"></image><button class="submit round abs-h-center" form-type="submit"></button></view></form></scroll-view><cu-modal vue-id="5" modalName="{{modalName}}" toastText="{{toastText}}" bind:__l="__l"></cu-modal></view>