<view class="bg_page_1" id="searchPage"><custom-nav vue-id="1" isBack="{{true}}" bind:__l="__l"></custom-nav><image class="img_bg abs-center" src="../../static/meetu-img/xingqiu.png" mode="aspectFill"></image><image class="img_change" src="../../static/meetu-img/huan.png" data-event-opts="{{[['tap',[['searchChange',['$event']]]]]}}" catchtap="__e"></image><view class="search_view"><block wx:if="{{isSearch}}"><view class="text-center text-white padding-top-lg">信号搜寻中, 请期待~~</view></block><block wx:for="{{searchValue}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['openStarHandle',['$0',index],[[['searchValue','',index]]]]]]]}}" hidden="{{!(item.isDisplay)}}" class="search_item animation-slide-bottom" style="{{'left:'+(item.left+'px')+';'+('top:'+(item.top+'px')+';')+('animation-delay:'+((index+1)*0.1+'s')+';')}}" bindtap="__e"><image class="img_star" src="../../static/meetu-img/star.png"></image><image class="img_avatar round abs-center" src="{{item.headimgurl}}" mode="aspectFill"></image></view></block></view><cu-modal vue-id="2" modalName="{{modalName}}" toastText="{{toastText}}" dialogText="{{dialogText}}" dialogSureText="{{dialogSureText}}" data-event-opts="{{[['^hideModal',[['hideModal']]],['^dialogConfirm',[['dialogConfirm']]]]}}" bind:hideModal="__e" bind:dialogConfirm="__e" bind:__l="__l" vue-slots="{{['modal']}}"><view slot="modal"><view class="open_star"><view class="action_one wh-100 padding-lg"><view class="user_info"><view class="avatar"><image class="wh-100 round" src="{{openItem.headimgurl}}" mode="aspectFill"></image><block wx:if="{{openItem.isvip==1}}"><image class="img_vip" src="../../static/meetu-img/vip.png" mode="aspectFill"></image></block></view><view><view class="name text-sm text-left text-letter-df"><text>{{openItem.nickname}}</text><image src="{{openItem.sex==1?'../../static/meetu-img/male.png':'../../static/meetu-img/famale.png'}}"></image></view><view class="text-xxs text-black-m text-left text-letter-df"><text>从 未知星球 发来信号</text></view></view></view><view class="star_info text-sm text-letter-df text-left"><block wx:if="{{openItem.type==1}}"><text>{{openItem.content}}</text></block><block wx:if="{{openItem.type==3}}"><image class="wh-100" src="{{openItem.content}}" mode="aspectFill"></image></block><block wx:if="{{openItem.type==2}}"><view data-event-opts="{{[['tap',[['playVoiceHandle',['$0'],['openItem.content']]]]]}}" class="voice flex-df" bindtap="__e"><image src="{{voicePlay?'../../static/meetu-img/xh.gif':'../../static/meetu-img/xh.png'}}" mode="aspectFill"></image><text>{{openItem.seconds+"s"}}</text></view></block></view><view class="btn"><button data-event-opts="{{[['tap',[['neglectHandle']]]]}}" class="cu-btn text-sm" catchtap="__e">忽略</button><button data-event-opts="{{[['tap',[['actionTwoToggle',['open']]]]]}}" class="cu-btn text-sm" catchtap="__e">回应</button></view></view><view class="{{['action_two wh-100',showActionTwo?'animation-right-in':'',!showActionTwo?'animation-right-out':'']}}"><form data-event-opts="{{[['submit',[['submitSendHandle',['$event']]]]]}}" bindsubmit="__e"><textarea name="sendtext" placeholder="请跟ta打个招呼吧～" data-event-opts="{{[['input',[['__set_model',['','textareaValue','$event',[]]]]]]}}" value="{{textareaValue}}" bindinput="__e"></textarea><view class="btn"><image class="back fl" src="../../static/meetu-img/back.png" data-event-opts="{{[['tap',[['actionTwoToggle',['close']]]]]}}" bindtap="__e"></image><button class="cu-btn fr submit" form-type="submit">发送</button></view></form></view></view></view></cu-modal></view>